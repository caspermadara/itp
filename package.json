{
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "postinstall": "node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js default"
  },
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "babelify": "^8.0.0",
    "bower": "^1.8.2",
    "browserify": "^16.0.0",
    "dotenv": "^5.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-pug": "^3.3.0",
    "gulp-stylus": "^2.7.0",
    "mysql2": "^1.5.2",
    "run-sequence": "^2.2.1",
    "rupture": "^0.7.1",
    "sequelize": "^4.33.1",
    "sequelize-cli": "^3.2.0",
    "serve-static": "^1.13.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "wiredep": "^4.0.0"
  }
}
